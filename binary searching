// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {             
    // Write C++ code here
     int arr []={1,2,3,4,6,7,8,9};          // binary searching element that is present in the array //
   int mid=-1;
    int start = 0;
    int end=7;
    int tar=9; 
    
while ( start <=  end)
{
     mid = ( start + end);
     
     if (tar == arr[mid])
     {
         cout << mid;
     }
     if ( tar < arr[mid])
     {
         end = mid -1;
     }
     if ( tar > mid)
     {
         start = mid + 1; 
     }
}

return 0;
}
